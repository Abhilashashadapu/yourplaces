# Environment variables for Vercel deployment
# Add these to your Vercel dashboard under Settings > Environment Variables

# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/yourplaces?retryWrites=true&w=majority

# JWT Secret for authentication
JWT_SECRET=your_super_secret_jwt_key_here

# Google Maps API Key (if needed for backend geocoding)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
